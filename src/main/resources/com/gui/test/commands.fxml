<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml" spacing="10.0">
    <HBox spacing="10.0">
        <ComboBox fx:id="comboBox" promptText="Выберите действие">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:id="add" fx:value="Добавить"/>
                    <String fx:id="remove" fx:value="Удалить"/>
                    <String fx:id="update" fx:value="Редактировать"/>
                    <String fx:id="save" fx:value="Сохранить"/>
                    <String fx:id="close" fx:value="Закрыть"/>
                </FXCollections>
            </items>
        </ComboBox>
        <Button text="Добавить"/>
    </HBox>
</VBox>