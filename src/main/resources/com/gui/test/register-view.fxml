<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<VBox prefHeight="400.0" prefWidth="300.0" alignment="CENTER" maxHeight="-Infinity"
      spacing="14.0"
      maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.gui.test.RegisterController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Text text="Register" style="-fx-font-weight: bold;">
        <font>
            <Font size="24.0"/>
        </font>
    </Text>
    <TextField fx:id="usernameField" onAction="#onEnter" promptText="Username"/>
    <TextField fx:id="nameField" onAction="#onEnter" promptText="Name"/>
    <PasswordField fx:id="passwordField" onAction="#onEnter" promptText="Password"/>
    <PasswordField fx:id="repeatPasswordField" onAction="#onEnter" promptText="repeatPassword"/>
    <Text fx:id="errorMessage" fill="#F00" wrappingWidth="260" textAlignment="CENTER"/>
    <Text fx:id="infoMessage" wrappingWidth="260" textAlignment="CENTER"/>
    <Button fx:id="registerButton" mnemonicParsing="false" onAction="#register"
            text="%register.register">
    </Button>
</VBox>